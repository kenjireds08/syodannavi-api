{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run vercel-build || npm run build",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
